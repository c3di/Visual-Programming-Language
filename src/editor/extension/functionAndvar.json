{
  "description": "A library for creating variables and functions",
  "enable": true,
  "nodes": {
    "Create Variable": {
      "type": "Create Variable",
      "category": "createVariable",
      "title": "New Variable",
      "tooltip": "Create a new variable",
      "sourceCode": "{{indent}}{{{inputs.1}}}: {{{inputs.2}}} = {{{inputs.3}}}\n{{{outputs.0}}}",
      "inputs": {
        "execIn": {
          "dataType": "exec",
          "tooltip": "exec in"
        },
        "name": {
          "title": "name",
          "tooltip": "name of the variable",
          "dataType": "string"
        },
        "type": {
          "title": "type",
          "tooltip": "type of the variable",
          "dataType": "extension1.Level2.module2.EDataType",
          "defaultValue": "string"
        },
        "value": {
          "title": "value",
          "tooltip": "value of the variable",
          "dataType": "string",
          "defaultValue": "string"
        }
      },
      "outputs": {
        "execOut": {
          "dataType": "exec"
        }
      }
    },

    "Setter": {
      "notShowInMenu": true,
      "type": "setter",
      "category": "setter",
      "sourceCode": "{{indent}}{{{inputsTitle.1}}} = {{{inputs.1}}}\n{{{outputs.0}}}"
    },

    "Getter": {
      "notShowInMenu": true,
      "type": "getter",
      "category": "getter",
      "tooltip": "Return the value",
      "sourceCode": "",
      "outputs": {
        "getter": {}
      }
    },

    "Create Function": {
      "type": "Create Function",
      "category": "createFunction",
      "sourceCode": "{{{outputs.0}}}",
      "outputs": {
        "execOut": {
          "title": "execOut",
          "dataType": "exec"
        }
      }
    },
    "functionCall": {
      "notShowInMenu": true,
      "type": "functionCall",
      "category": "functionCall"
    },
    "Return": {
      "title": "Return",
      "type": "Return",
      "category": "return",
      "showLabel": true,
      "tooltip": "Return",
      "sourceCode": "{{indent}}return {{#inputs}}{{#.}}{{{.}}}, {{/.}}{{/inputs}}",
      "inputs": {
        "execIn": {
          "title": "execIn",
          "dataType": "exec"
        }
      }
    }
  }
}
